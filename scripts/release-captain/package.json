{
  "name": "release-captain",
  "version": "1.0.0",
  "description": "Interactive CLI tool to help Cody release captains manage the release process",
  "main": "release-captain.ts",
  "bin": {
    "release-captain": "./dist/release-captain.js"
  },
  "scripts": {
    "start": "ts-node release-captain.ts",
    "build": "tsc",
    "lint": "eslint .",
    "dev": "ts-node --transpile-only release-captain.ts",
    "dry-run": "ts-node --transpile-only release-captain.ts start --dry-run"
  },
  "dependencies": {
    "@types/chalk": "^2.2.4",
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "inquirer": "^8.2.5"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.5",
    "@types/node": "^18.11.18",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
